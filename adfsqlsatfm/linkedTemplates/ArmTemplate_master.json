{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adfsqlsatfm"
		},
		"blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob'"
		},
		"AzureBlobStorageSinkFileStore1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"AzureBlobStorageSinkFileStore1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().folder"
		},
		"AzureBlobStorageSourceFileStore1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().folder"
		},
		"airline_bzip_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@{dataset().file_pattern}.csv.bz2"
		},
		"airline_bzip_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "airline/bzip"
		},
		"airline_csv_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@{dataset().file_pattern}.csv.bz2"
		},
		"airline_csv_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "airline/csv"
		},
		"source_files_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "airline/bzip"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blob_connectionString": {
						"value": "[parameters('blob_connectionString')]"
					},
					"AzureBlobStorageSinkFileStore1_properties_typeProperties_fileName": {
						"value": "[parameters('AzureBlobStorageSinkFileStore1_properties_typeProperties_fileName')]"
					},
					"AzureBlobStorageSinkFileStore1_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureBlobStorageSinkFileStore1_properties_typeProperties_folderPath')]"
					},
					"AzureBlobStorageSourceFileStore1_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureBlobStorageSourceFileStore1_properties_typeProperties_folderPath')]"
					},
					"airline_bzip_properties_typeProperties_fileName": {
						"value": "[parameters('airline_bzip_properties_typeProperties_fileName')]"
					},
					"airline_bzip_properties_typeProperties_folderPath": {
						"value": "[parameters('airline_bzip_properties_typeProperties_folderPath')]"
					},
					"airline_csv_properties_typeProperties_fileName": {
						"value": "[parameters('airline_csv_properties_typeProperties_fileName')]"
					},
					"airline_csv_properties_typeProperties_folderPath": {
						"value": "[parameters('airline_csv_properties_typeProperties_folderPath')]"
					},
					"source_files_properties_typeProperties_folderPath": {
						"value": "[parameters('source_files_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}